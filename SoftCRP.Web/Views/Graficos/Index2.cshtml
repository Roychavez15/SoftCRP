                                style: {
                                    fontSize: '10px',
                                    fontFamily: 'Verdana, sans-serif'
                                }
                            }
                        }]
                    });
                    //frenazos bruscos
                    chart4 = new Highcharts.Chart({
                        chart: {
                            renderTo: 'container4',
                            type: 'column',
                            options3d: {
                                enabled: true,
                                alpha: parseFloat($("#alpha").val()),
