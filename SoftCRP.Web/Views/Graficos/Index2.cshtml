    <script type="text/javascript">
        window.onload = function () {
            labelserie = false;
            $("#container").hide();
            $("#container1").hide();
            $("#container2").hide();
            $("#container3").hide();
            $("#container4").hide();
            $("#container5").hide();
            $("#container6").hide();
            $("#container7").hide();
            MostrarOcultarDiv();

            $("#Proceso").change(function () {
                $("#loaderbody").removeClass('hide');
                
                labelserie = false;
                var x = $("#Proceso").val();
                $("#SubProceso").empty();
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("GetSubProcess")',
                    dataType: 'json',
                    data: { proceso: $("#Proceso").val() },
                    success: function (subs) {
                        $("#SubProceso").append('<option value="0">(Seleccionar...)</option>');
                        $.each(subs, function (i, sub) {
                            $("#SubProceso").append('<option value="'
                                + sub.value + '">'
                                + sub.text + '</option>');
                        });
                    },
                    error: function (ex) {
                        alert('Failed to retrieve subprocesos.' + ex.statusText);
                    }
                });
                

                //clientes
                $("#Cliente").empty();
                $("#Placa").empty();
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("GetClientes")',
                    dataType: 'json',
                    //data: { proceso: $("#Proceso").val() },
                    success: function (subs) {
                        $("#Cliente").append('<option value="0">(Seleccionar...)</option>');
                        $.each(subs, function (i, sub) {
                            $("#Cliente").append('<option value="'
                                + sub.value + '">'
                                + sub.text + '</option>');
                        });
                    },
                    error: function (ex) {
                        alert('Failed to retrieve Clientes.' + ex.statusText);
                    }
                });
               
                MostrarOcultarDiv();
                GenerateRuntimeGraph();
                $("#loaderbody").addClass('hide');
                return false;
            });
            //subprocesos
            $("#SubProceso").change(function () {
                labelserie = false;
                $("#loaderbody").removeClass('hide');
                MostrarOcultarDiv();
                $("#loaderbody").addClass('hide');
            });
            ////////////
            $("#Cliente").change(function () {
                MostrarOcultarDiv();
                if ($("#Cliente").val() != "(Seleccionar...)") {
                    labelserie = true;
                }
                
                $("#loaderbody").removeClass('hide');
                var x = $("#Cliente").val();
                $("#Placa").empty();
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("GetPlacas")',
                    dataType: 'json',
